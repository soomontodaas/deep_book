}
@Article{RePEc:inm:oropre:v:13:y:1965:i:5:p:857-859,
  author={Robert L. Helmbold},
  title={{Letter to the Editor—A Modification of Lanchester's Equations}},
  journal={Operations Research},
  year=1965,
  volume={13},
  number={5},
  pages={857-859},
  month={October},
  keywords={},
  doi={10.1287/opre.13.5.857},
  abstract={The familiar Lanchester square law attrition model assumes that the attrition produced by a force is independent of enemy force size. This assumption may require modification if the larger force suffers inefficiencies of scale when force sizes are grossly unequal. A modification that alters relative force attrition capability by a factor depending on the force ratio is introduced and discussed in terms of a special case.},
  url={https://ideas.repec.org/a/inm/oropre/v13y1965i5p857-859.html}
}
@Article{JohsonMackey:2011,
  author={Ian R. Johnson and Niall J. MacKay},
  title={{Lanchester models and the battle of Britain}},
  journal={Naval Research Logistics (NRL)},
  year=2011,
  volume={58},
  number={3},
  pages={210-222},
  keywords={},
  doi={10.1002/nav.20328},
  abstract={We fit deterministic generalized Lanchester models to daily sortie and loss data from the Battle of Britain. The best fit for the period 14th August to 30th October 1940 is δB ∼ G1.2, δG ∼ G0.9, where B and G are RAF Fighter Command and Luftwaffe sortie numbers, and δB and δG are daily loss numbers, respectively. The data naturally divide into two phases, with losses (as a proportion of overall sortie numbers) much reduced after 15th September. Fits were generally better for the first phase than for the second, and for British losses than for German; in every case the dependence on G is stronger than that on B. Days with higher sortie numbers on average favored the Luftwaffe, whereas the loss‐ratio was not significantly dependent on the force ratio. © 2008 Wiley Periodicals, Inc. Naval Research Logistics 58: 210–222, 2011},
  url={https://ideas.repec.org/a/wly/navres/v58y2011i3p210-222.html}
}
