@article{Moraled90c4876-e2f0-39fb-b021-9e98ca8fbee3,
 ISSN = {00472697, 26422190},
 URL = {http://www.jstor.org/stable/45293180},
 abstract = {The concept of morale has acquired a large variety of definitions, including a job satisfaction oriented definition that has recently gained prominence in the military. Using this definition, this paper attempts to answer two questions. First, what is the precise relationship between morale, as defined in terms of the soldier's satisfaction with his work environment, and proficiency and discipline? Second, how much impact does the soldier's unit have on his morale? After measuring and analyzing the morale, proficiency, and discipline of approximately four hundred soldiers in a number of combat arms units, this study concludes that morale is significantly related to proficiency and discipline, particularly for those soldiers with extremely high or low morale. However, morale does not appear to be significantly affected by the soldier's unit, but is rather determined by other factors, such as his long-term attitudes and values.},
 author = {STEPHEN D. WESBROOK},
